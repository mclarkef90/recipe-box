<h1>Welcome to Recipe Box!</h1>

<h3>All Recipes</h3>

  <ul>
    <% @genres.each do |genre| %>
      <li> <%=genre.genre_name %> </li>
      <% genre.recipes.each do |recipe| %>
        <ul><li><a href= "/recipes/<%=recipe.id%>"> <%= recipe.name %> </a> </li></ul> <br>
         <% end %>
    <% end %>
  </ul>
