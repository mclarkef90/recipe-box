<h2>Your Recipes</h2>

  <h3>Click a Recipe Below to View and/or Update</h3>

<ul>
  <% @genres.each do |genre| %>


  <li>  <%=genre.genre_name %></li>

   <% genre.recipes.each do |recipe| %>
    <% if recipe.user == current_user %>
  <ul><li>  <a href= "/recipes/<%=recipe.id%>"> <%= recipe.name %> </a> </li></ul><br>
              <% end %>
              <% end %>
              <% end %>
